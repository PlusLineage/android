<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!--Lineage+ -->
  <remote  name="plus"
           fetch="https://github.com/PlusLineage"
           revision="plus" />

  <!-- GitLab -->
  <remote name="gitlab"
           fetch="https://gitlab.com" />

  <!-- Removed projects -->
  <remove-project name="LineageOS/android_frameworks_base" />
  <remove-project name="LineageOS/android_packages_apps_Settings" />
  <remove-project name="LineageOS/android_vendor_lineage" />
  <remove-project name="LineageOS/android_packages_apps_Updater" />

  <!-- Our forks -->
  <project path="frameworks/base" name="frameworks_base" remote="plus" />
  <project path="vendor/lineage" name="vendor_lineage" remote="plus" />
  <project path="packages/apps/Settings" name="packages_apps_Settings" remote="plus" />
  <project path="packages/apps/Updater" name="packages_apps_Updater" remote="plus" />

</manifest>
