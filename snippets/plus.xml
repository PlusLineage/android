<manifest>
<!--Lineage+ -->
  <remote  name="plus"
           fetch="https://github.com/PlusLineage"
           revision="plus" />

  <!-- Github -->
  <remote name="gh"
           fetch="https://github.com/" />

  <!-- GitLab -->
  <remote name="gitlab"
           fetch="https://gitlab.com" />

  <!-- Pixel -->
  <remote name="pixel-gitlab-self-hosted"
          fetch="https://gitlab.pixelexperience.org/android"
          revision="twelve" />

  <remote name="pixel"
          fetch="https://github.com/PixelExperience"
          revision="twelve"
          review="gerrit.pixelexperience.org" />

  <!-- Removed projects -->
  <remove-project name="LineageOS/android_frameworks_base" />
  <remove-project name="LineageOS/android_packages_apps_Settings" />
  <remove-project name="LineageOS/android_packages_apps_Updater" />
  <remove-project name="LineageOS/android_vendor_lineage" />
  <remove-project name="LineageOS/android_system_core" />
  <remove-project name="LineageOS/android_lineage-sdk" />

  <!-- Our forks -->
  <project path="frameworks/base" name="frameworks_base" remote="plus" />
  <project path="packages/apps/GameSpace" name="packages_apps_GameSpace" remote="plus" />
  <project path="packages/apps/Settings" name="packages_apps_Settings" remote="plus" />
  <project path="packages/apps/Updater" name="packages_apps_Updater" remote="plus" />
  <project path="vendor/lineage" name="vendor_lineage" remote="plus" />
  <project path="system/core" name="system_core" remote="plus" />
  <project path="lineage-sdk" name="lineage-sdk" remote="plus" />
  <project path="vendor/lawnchair" name="vendor_lawnchair" remote="plus" />

  <!--Extras -->
  <project path="external/faceunlock" name="external_faceunlock" remote="pixel-gitlab-self-hosted" />
  <project path="packages/apps/RepainterServicePriv" name="kdrag0n/android_packages_apps_RepainterServicePriv" remote="github" revision="main" />
  <project path="packages/apps/FaceUnlockService" name="packages_apps_FaceUnlockService" remote="pixel" />
  <project path="vendor/gms" name="EvoX/vendor_gapps" remote="gitlab" revision="snow" />
  <project path="vendor_PixelLauncher" name="Ardjlon/vendor_PixelLauncher" remote="github" revision="ammit" />
  
</manifest>
